#include<stdio.h>

int sumofdig(int num)
{
    int m,sum=0;
    while(num>0)
    {
        m=num%10;
        sum=sum+m;
        num=num/10;
        
    }
    return sum;
}


int main()
{   
    int num,result;
    scanf("%d",&num);
    result=sumofdig(num);
    printf("%d",result);
    return 0;
}
